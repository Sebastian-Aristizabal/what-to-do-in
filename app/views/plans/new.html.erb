

<h2 class="title-form">¡¡Nuevo parche!! </h2>
<div class="contenedor_newplans">
      <%=  simple_form_for @plan do |f| %>
        <%= f.input :title %>
        <%= f.input :video_url %>
        <%= f.input :details %>
        <%= f.input :start_date %>
        <%= f.input :end_date %>
        <%= f.input :start_hour %>
        <%= f.input :end_hour %>
        <%= f.input :address %>
        <%= f.input :link_web %>
        <%= f.input :instagram_link %>
        <%= f.input :facebook_link %>
        <%# <%= f.input :category_id, collection: [Category.order(:name)], as: :radio_buttons, wrapper: :vertical_collection_inline  %>
        <%= f.collection_select :category_id, Category.order(:name), :id, :name  %>
        <%= f.input :latitude %>
        <%= f.input :longitude %>
        <%= f.input :free %>
        <%= f.submit "submit", class:"btn btn-primary" %>
      <% end %>
</div>

    <%# t.string "title"
    t.string "video_url"
    t.text "details"
    t.datetime "start_date", precision: nil
    t.datetime "end_date", precision: nil
    t.datetime "start_hour", precision: nil
    t.datetime "end_hour", precision: nil
    t.string "address"
    t.string "link_web"
    t.string "instagram_link"
    t.string "facebook_link"
    t.datetime "created_at", null: false
    t.datetime "updated_at", null: false
    t.bigint "category_id", null: false
    t.bigint "user_id", null: false
    t.float "latitude"
    t.float "longitude"
    t.boolean "free" %>
